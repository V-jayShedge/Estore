<% products.forEach(function(product) { %>
    <div class="container-fluid padding" id="products">
    <div class="row padding">
        <% product.forEach(function(prod) { %>
            
        <div class="col-md-4 p-2">
            <div class="card" >
           <img src="./img/<%= prod.image %>" class="img-fluid " alt="image" style="height: 250px !important;">
            <div class="card-body">
           <h3 class="card-title"><%= prod.pname  %></h3>
           <p class="card-text"><%= prod.description  %></p>
           <a href="/cart/addtocart/<%= prod.pname %>"><button class="btn btn-success">Add to cart</button></a>
           <h3>Rs<%= prod.price  %></h3>
            </div>
           </div>
        </div>  
        <% }) %>
        </div>
        </div>
<% }) %>

